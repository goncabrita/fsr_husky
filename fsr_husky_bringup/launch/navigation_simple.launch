<launch>

	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
		<rosparam file="$(find fsr_husky_bringup)/settings/odom_navigation_parameters/costmap_common_params.yaml" command="load" ns="global_costmap" />
		<rosparam file="$(find fsr_husky_bringup)/settings/odom_navigation_parameters/costmap_common_params.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find fsr_husky_bringup)/settings/odom_navigation_parameters/local_costmap_params.yaml" command="load" />
		<rosparam file="$(find fsr_husky_bringup)/settings/odom_navigation_parameters/global_costmap_params.yaml" command="load" />
		<rosparam>
        TrajectoryPlannerROS:
            xy_goal_tolerance: 0.5
            yaw_goal_tolerance: 0.1
    </rosparam>
  </node>
</launch>